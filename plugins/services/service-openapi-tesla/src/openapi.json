    "components": {
        "schemas": {
            "LoginFailure": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "boolean"
                    },
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "region": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "data": {
                        "type": "object",
                        "properties": {}
                    },
                    "homeRegion": {
                        "type": "string"
                    }
                }
            },
            "VehicleState": {
                "type": "object",
                "properties": {
                    "sun_roof_installed": {
                        "type": "boolean",
                        "description": "panoramic roof is installed",
                        "example": false
                    },
                    "df": {
                        "type": "boolean",
                        "description": "driver's side front door open",
                        "example": false
                    },
                    "pr": {
                        "type": "boolean",
                        "description": "passenger's side rear door open",
                        "example": false
                    },
                    "rt": {
                        "type": "boolean",
                        "description": "rear trunk is open",
                        "example": false
                    },
                    "dark_rims": {
                        "type": "boolean",
                        "description": "gray rims installed",
                        "example": false
                    },
                    "locked": {
                        "type": "boolean",
                        "description": "car is locked",
                        "example": true
                    },
                    "dr": {
                        "type": "boolean",
                        "description": "driver's side rear door open",
                        "example": false
                    },
                    "ft": {
                        "type": "boolean",
                        "description": "front trunk is open",
                        "example": false
                    },
                    "pf": {
                        "type": "boolean",
                        "description": "passenger's side front door open",
                        "example": false
                    },
                    "roof_color": {
                        "type": "string",
                        "description": "None for panoramic roof",
                        "example": "Colored"
                    },
                    "perf_config": {
                        "type": "string",
                        "example": "Base"
                    },
                    "sun_roof_state": {
                        "type": "string",
                        "example": "unknown"
                    },
                    "car_verson": {
                        "type": "string",
                        "description": "car firmware version",
                        "example": "1.19.42"
                    },
                    "sun_roof_percent_open": {
                        "type": "integer",
                        "description": "null if not installed",
                        "format": "int32",
                        "example": 0
                    },
                    "has_spoiler": {
                        "type": "boolean",
                        "description": "spoiler is installed",
                        "example": false
                    },
                    "wheel_type": {
                        "type": "string",
                        "description": "wheel type installed",
                        "example": "Base19"
                    }
                }
            },
            "VehicleStateResponse": {
                "type": "object",
                "properties": {
                    "response": {
                        "$ref": "#/components/schemas/VehicleState"
                    }
                }
            },
            "DriveState": {
                "type": "object",
                "properties": {
                    "shift_state": {
                        "type": "string"
                    },
                    "speed": {
                        "type": "number",
                        "format": "double"
                    },
                    "latitude": {
                        "type": "number",
                        "description": "degrees north of equator",
                        "format": "double",
                        "example": 37.4292
                    },
                    "longitude": {
                        "type": "number",
                        "description": "degrees west of the prime meridian",
                        "format": "double",
                        "example": 122.1381
                    },
                    "heading": {
                        "maximum": 359,
                        "minimum": 0,
                        "type": "integer",
                        "description": "integer compass heading",
                        "format": "int32",
                        "example": 6
                    },
                    "gps_as_of": {
                        "type": "object",
                        "example": "1359865301"
                    }
                }
            },
            "VehiclesResponse": {
                "type": "object",
                "properties": {
                    "response": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Vehicle"
                        }
                    },
                    "count": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "DriveStateResponse": {
                "type": "object",
                "properties": {
                    "response": {
                        "$ref": "#/components/schemas/DriveState"
                    }
                }
            },
            "BooleanResponse": {
                "type": "object",
                "properties": {
                    "response": {
                        "type": "boolean"
                    }
                }
            },
            "Vehicle": {
                "type": "object",
                "properties": {
                    "color": {
                        "type": "string"
                    },
                    "display_name": {
                        "type": "string"
                    },
                    "vehicle_id": {
                        "type": "string",
                        "example": "1234567890"
                    },
                    "user_id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 123
                    },
                    "state": {
                        "type": "string",
                        "example": "online"
                    },
                    "tokens": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "vin": {
                        "type": "string",
                        "example": "5YJSA1CN5CFP01657"
                    },
                    "id": {
                        "type": "integer",
                        "format": "int32",
                        "example": 321
                    },
