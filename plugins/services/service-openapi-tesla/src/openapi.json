        "/oauth/token": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "operationId": "login",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "required": [
                                    "client_id",
                                    "email",
                                    "grant_type",
                                    "password"
                                ],
                                "properties": {
                                    "grant_type": {
                                        "type": "string",
                                        "default": "password"
                                    },
                                    "client_id": {
                                        "type": "string",
                                        "default": "e4a9949fcfa04068f59abb5a658f2bac0a3428e4652315490b659d5ab3f35a9e"
                                    },
                                    "client_secret": {
                                        "type": "string",
                                        "default": "c75f14bbadc8bee3a7594412c31416f8300256d7668ea7e6e7f06727bfb9d220"
                                    },
                                    "email": {
                                        "type": "string",
                                        "description": "the user email address"
                                    },
                                    "password": {
                                        "type": "string",
                                        "description": "the password",
                                        "format": "password"
                                    }
                                }
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "success",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginSuccess"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "failed login",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginFailure"
                                }
                            }
                        }
                    }
                }
            }
        },
